{
  "name": "react-rogue",
  "version": "1.3.0",
  "homepage": "https://albertoPravisano.github.io/react-rogue",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rot-js": "^2.2.0"
  },
  "devDependencies": {
    "gh-pages": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "publish:patch": "npm version patch && git push && git push --tags && npm run deploy",
    "publish:minor": "npm version minor && git push && git push --tags && npm run deploy",
    "publish:major": "npm version major && git push && git push --tags && npm run deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
